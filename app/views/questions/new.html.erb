<dl class="sub-nav">
  <dd class="active"><%= link_to '_home', root_path %></dd>
</dl>

<div class="title_header">
  _new question
</div>

<fieldset>
  <%= form_for @question do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.submit "Add Question" %>
  <% end %>
</fieldset>
